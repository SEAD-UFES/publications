{
  "name": "publications",
  "version": "1.0.0",
  "description": "sead publications backend system",
  "main": "server.js",
  "scripts": {
    "migrate":  "./node_modules/.bin/sequelize db:migrate:undo:all; ./node_modules/.bin/sequelize db:migrate",
    "seed":     "./node_modules/.bin/sequelize db:seed:undo:all; ./node_modules/.bin/sequelize db:seed:all",
    "refresh":  "npm run migrate; npm run seed",
    "report": "nyc --reporter=html --reporter=text mocha --exit ./test/*.test.js",
    "start": "nodemon server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SEAD-UFES/publications.git"
  },
  "author": "wagner perin, eduardo santos",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/SEAD-UFES/publications/issues"
  },
  "homepage": "https://github.com/SEAD-UFES/publications#readme",
  "dependencies": {
    "bcrypt": "^3.0.4",
    "body-parser": "^1.18.3",
    "consign": "^0.1.6",
    "express": "^4.16.4",
    "jsonwebtoken": "^8.4.0",
    "multer": "^1.4.1",
    "mysql2": "^1.6.5",
    "npm": "^6.8.0",
    "sequelize": "^4.42.0",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "cross-env": "^5.2.0",
    "expect": "^24.1.0",
    "mocha": "^5.2.0",
    "nodemon": "^1.18.10",
    "nyc": "^13.3.0",
    "sequelize-cli": "^4.1.1",
    "supertest": "^3.4.2"
  }
}
